{
  "family": "zighangbok",
  "taskRoleArn": "arn:aws:iam::340547707424:role/ZighangbokTaskRole",
  "executionRoleArn": "arn:aws:iam::340547707424:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "zighangbok",
      "image": "340547707424.dkr.ecr.ap-northeast-2.amazonaws.com/zighangbok:latest",
      "cpu": 512,
      "portMappings": [
        {
          "name": "zighangbok-8080-tcp",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/zighangbok",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "name": "redis",
      "image": "redis:7",
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 6379,
          "protocol": "tcp"
        }
      ],
      "essential": false,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/zighangbok",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "redis"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "ephemeralStorage": {
    "sizeInGiB": 21
  },
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "volumes": [],
  "placementConstraints": []
}

